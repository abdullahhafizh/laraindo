{
    "name": "afrizalmy/laraindo",
    "description": "Package Laravel sederhana untuk format indonesia terdiri dari Localization, Rupiah format, Date format",
    "type": "library",
    "keywords": [
        "Localization",
        "Rupiah",
        "Tanggal",
        "laravel",
        "indo",
        "indonesia"
    ],
    "require": {
        "php": ">=8.0.2",
        "laravel/framework": "^9.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Laraindo\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laraindo\\LaraindoServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laraindo\\Test\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Afrizal",
            "email": "afrizalyasin423@gmail.com",
            "homepage": "https://github.com/afrizal423",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "afrizalyasin423@gmail.com",
        "issues": "https://github.com/afrizal423/laraindo/issues",
        "source": "https://github.com/afrizal423/laraindo",
        "docs": "https://github.com/afrizal423/laraindo/README.md"
    },
    "minimum-stability": "dev",
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
